{
    "name": "alto/favicon",
    "description": "Generate a modern favicon set (SVG icon, apple-touch-icon, favicon.ico, Web Manifest) from a single SVG or PNG file.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "favicon",
        "generator",
        "icon",
        "svg",
        "png",
        "ico",
        "manifest",
        "webmanifest",
        "apple-touch-icon",
        "cli",
        "php",
        "php-cli",
        "alto"
    ],
    "authors": [
        {
            "name": "Simon AndrÃ©",
            "email": "smn.andre@gmail.com"
        }
    ],
    "homepage": "https://github.com/phpalto/favicon",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/smnandre"
        }
    ],
    "require": {
        "php": ">=8.3",
        "psr/log": "^3.0",
        "symfony/console": "^7.3 || ^8.0",
        "symfony/filesystem": "^7.3 || ^8.0",
        "symfony/process": "^7.3 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.5"
    },
    "suggest": {
        "ext-gd": "Required for PNG resizing if Imagick is not available",
        "ext-imagick": "Recommended for better quality PNG resizing"
    },
    "autoload": {
        "psr-4": {
            "Alto\\Favicon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alto\\Favicon\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/favicon"
    ],
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix",
        "qa": [
            "@cs",
            "@sa",
            "@test"
        ],
        "sa": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
